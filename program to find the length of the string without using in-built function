import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String a = sc.nextLine();
        int u=0;
        for(int i=0;i<a.length();i++){
            u++;
        }
         System.out.println(u);
    }
}
